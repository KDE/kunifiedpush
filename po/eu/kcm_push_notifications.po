#, kde-format
msgctxt "@action:button"
msgid "Test"
msgstr "Proba"

#: ui/main.qml:334
#, kde-format
msgid "Applications"
msgstr "Aplikazioak"

#: ui/main.qml:364
#, kde-format
msgid "Unregister application from push notifications"
msgstr "Kendu aplikazio 'push' jakinarazpenen erregistrotik"

#: ui/main.qml:371

#: selftest.cpp:34
#, kde-format
msgid "The operation timed out."
msgstr "A művelet időtúllépés miatt megszakadt."

#: selftest.cpp:59
#, kde-format
msgid "Push notification self-test."
msgstr "Leküldéses értesítés önellenőrzés."

#: selftest.cpp:73
#, kde-format
msgid "Could not obtain push notification endpoint."
msgstr "Nem sikerült elérni a leküldéses értesítés végpontját."

#: selftest.cpp:126
#, kde-format
msgid "Received notification does not have the expected content."
msgstr "A kapott értesítés nem a várt tartalommal rendelkezik."

#: ui/main.qml:28
#, kde-format
msgid "There is no push notification service running!"
msgstr "Nincs futó leküldéses értesítési szolgáltatás."

#: ui/main.qml:37
#, kde-format
msgid ""
"There is a 3rd party push notification service running. Push notifications "
"are available, but cannot be configured here."
msgstr ""
"Egy harmadik féltől származó leküldéses értesítési szolgáltatás fut. A "
"leküldéses értesítések elérhetők, de itt nem konfigurálhatók."

#: ui/main.qml:46
#, kde-format
msgid ""
"<b>Online</b><br>Connected to the push notification server and operational."
msgstr ""
"<b>Online</b><br>Kapcsolódva a leküldéses értesítési kiszolgálóhoz és "
"működőképes."

#: ui/main.qml:55
#, kde-format
msgid "<b>Idle</b><br>There are no applications using push notifications."
msgstr ""
"<b>Tétlen</b><br>Nincsenek leküldéses értesítéseket használó alkalmazások."

#: ui/main.qml:65
#, kde-format
msgid ""
"<b>Offline</b><br>Network connection to the server could not be established "
"(%1)."
msgstr ""
"<b>Offline</b><br>Nem sikerült hálózati kapcsolatot létesíteni a "
"kiszolgálóval (%1)."

#: ui/main.qml:66
#, kde-format
msgid ""
"<b>Offline</b><br>Network connection to the server could not be established."
msgstr ""
"<b>Offline</b><br>Nem sikerült hálózati kapcsolatot létesíteni a "
"kiszolgálóval."

#: ui/main.qml:76
#, kde-format
msgid "<b>Offline</b><br>Could not authenticate at the server (%1)."
msgstr "<b>Offline</b><br>Nem sikerült hitelesíteni a kiszolgálót (%1)."

#: ui/main.qml:77
#, kde-format
msgid "<b>Offline</b><br>Could not authenticate at the server."
msgstr "<b>Offline</b><br>Nem sikerült hitelesíteni a kiszolgálót."

#: ui/main.qml:86
#, kde-format
msgid "<b>Offline</b><br>Push notifications are not set up yet."
msgstr "<b>Offline</b><br>A leküldéses értesítések még nincsenek beállítva."


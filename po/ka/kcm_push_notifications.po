#: selftest.cpp:34
#, kde-format
msgid "The operation timed out."
msgstr "ოპერაციის მოლოდინის ვადა ამოიწურა."

#: selftest.cpp:59
#, kde-format
msgid "Push notification self-test."
msgstr "პუშ გაფრთხილების თვითშემოწმება."

#: selftest.cpp:73
#, kde-format
msgid "Could not obtain push notification endpoint."
msgstr "პუშ-გაფრთხილების ბოლოწერტილის მიღება შეუძლებელია."

#: selftest.cpp:126
#, kde-format
msgid "Received notification does not have the expected content."
msgstr "მიღებულ გაფრთხილებას მოსალოდნელი შემცველობა არ აქვს."

#: ui/main.qml:28
#, kde-format
msgid "There is no push notification service running!"
msgstr "პუშ გაფრთხილებების სერვისი გაშვებული არაა!"

#: ui/main.qml:37
#, kde-format
msgid ""
"There is a 3rd party push notification service running. Push notifications "
"are available, but cannot be configured here."
msgstr ""
"გაშვებულია პუშ გაფრთხილებების მესამე პირების მიერ მოწოდებული სერვისი. პუშ "
"გაფრთხილებები ხელმისაწვდომია, მაგრამ მის პარამეტრებს აქ ვერ შეცვლით."

#: ui/main.qml:46
#, kde-format
msgid ""
"<b>Online</b><br>Connected to the push notification server and operational."
msgstr "<b>ხაზზე</b><br> მივუერთდი პუშ გაფრთხილებების სერვერს და ვმუშაობ."

#: ui/main.qml:55
#, kde-format
msgid "<b>Idle</b><br>There are no applications using push notifications."
msgstr "<b>უქმე</b><br>პუშ გაფრთხილებებს არც ერთი აპლიკაცია არ იყენებს.."

#: ui/main.qml:65
#, kde-format
msgid ""
"<b>Offline</b><br>Network connection to the server could not be established "
"(%1)."
msgstr "<b>ქსელის გარეშე</b><br>სერვერთან (%1) ქსელური მიერთება შეუძლებელია."

#: ui/main.qml:66
#, kde-format
msgid ""
"<b>Offline</b><br>Network connection to the server could not be established."
msgstr "<b>ოფლაინ</b><br>სერვერთან ქსელური მიერთება შეუძლებელია."

#: ui/main.qml:76
#, kde-format
msgid "<b>Offline</b><br>Could not authenticate at the server (%1)."
msgstr "<b>ქსელის გარეშე</b><br>სერვერთან (%1) ავთენტიკაციის შეცდომა."

#: ui/main.qml:77
#, kde-format
msgid "<b>Offline</b><br>Could not authenticate at the server."
msgstr "<b>ქსელის გარეშე</b><br>სერვერთან ავთენტიკაციის შეცდომა."

#: ui/main.qml:86
#, kde-format
msgid "<b>Offline</b><br>Push notifications are not set up yet."
msgstr "<b>ქსელის გარეშე</b><br>პუშ გაფრთხილებები ჯერ მორგებული არაა."

#: ui/main.qml:114
#, kde-format
msgid "Push provider:"
msgstr "პუშის მომწოდებელი:"

#: ui/main.qml:155 ui/main.qml:182 ui/main.qml:231 ui/main.qml:250
#, kde-format
msgid "Url:"
msgstr "Url:"

#: ui/main.qml:160
#, kde-format
msgid "Client token:"
msgstr "კლიენტის კოდი:"

#: ui/main.qml:187
#, kde-format
msgid "User name:"
msgstr "მომხმარებელი:"

#: ui/main.qml:193
#, kde-format
msgid "Authenticate"
msgstr "ავთენტიკაცია"

#: ui/main.qml:295
#, kde-format
msgid "Push notifications are working correctly."
msgstr "პუშ-გაფრთხილებები სწორად მუშაობს."

#: ui/main.qml:297
#, kde-format
msgid "Push notification test failed."
msgstr "პუშ-გაფრთხილებების შემოწმება ჩავარდა."

#: ui/main.qml:299
#, kde-format
msgid "Registering with push server…"
msgstr "რეგისტრაცია პუშ-სერვერთან…"

#: ui/main.qml:301
#, kde-format
msgid "Sending push notification…"
msgstr "პუშ-გაფრთხილების გაგზავნა…"

#: ui/main.qml:303
#, kde-format
msgid "Waiting to receive push notification…"
msgstr "პუშ-გაფრთხილების მიღების მოლოდინი…"

#: ui/main.qml:310
#, kde-format
msgctxt "@action:button"
msgid "Test"
msgstr "შემოწმება"

#: ui/main.qml:327
#, kde-format
msgid "Applications"
msgstr "აპლიკაციები"

#: ui/main.qml:357
#, kde-format
msgid "Unregister application from push notifications"
msgstr "პუშ გაფრთხილებებიდან აპლიკაციის რეგისტრაციის მხოსნა"

#: ui/main.qml:364
#, kde-format
msgctxt "@title:window"
msgid "Unregister Application"
msgstr "აპლიკაციის რეგისტრაციის გაუქმება"

#: ui/main.qml:365
#, kde-format
msgctxt "%1 is an application name"
msgid "Are you sure you want to unregister '%1'?"
msgstr "დარწმუნებული ბრძანდებით, რომ გნებავთ, გააუქმოთ რეგისტრაცია '%1'-სთვის?"

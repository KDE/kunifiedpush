#, kde-format
msgid "Client token:"
msgstr "Token klienta:"

#: package/contents/ui/main.qml:134
#, kde-format
msgid "User name:"
msgstr "Nazwa użytkownika:"

#: package/contents/ui/main.qml:140
#, kde-format
msgid "Authenticate"
msgstr "Uwierzytelnij"

#: package/contents/ui/main.qml:213
#, kde-format
msgid "Applications"
msgstr "Uwierzytelnienia"

#: package/contents/ui/main.qml:236
#, kde-format
msgid "Unregister application from push notifications"
msgstr "Wypisz aplikację z powiadomień push"

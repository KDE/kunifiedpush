#, kde-format
msgid "Client token:"
msgstr "Токен клиента:"

#: package/contents/ui/main.qml:134
#, kde-format
msgid "User name:"
msgstr "Имя пользователя:"

#: package/contents/ui/main.qml:140
#, kde-format
msgid "Authenticate"
msgstr "Аутентификация"

#: package/contents/ui/main.qml:213
#, kde-format
msgid "Applications"
msgstr "Приложения"

#: package/contents/ui/main.qml:236
